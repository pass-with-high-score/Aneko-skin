<?xml version="1.0" encoding="utf-8"?>
<motion-params
        package="org.nqmgaming.skitty"
        name="Skitty"
        preview="icon.png"
        author="nqmgaming"
        acceleration="100"
        awakeState="awake"
        deaccelerationDistance="60"
        initialState="stop"
        maxVelocity="50"
        moveStatePrefix="move"
        proximityDistance="10">
    <motion state="stop">
        <repeat-item>
            <item
                    drawable="down3"
                    duration="5000" />
            <item
                    drawable="down2"
                    duration="2000" />
            <repeat-item repeatCount="6">
                <item
                        drawable="arotate1"
                        duration="1000" />
                <item
                        drawable="arotate2"
                        duration="1000" />
            </repeat-item>
            <item
                    drawable="down3"
                    duration="1000" />
            <item
                    drawable="arotate7"
                    duration="1000" />
            <item
                    drawable="down3"
                    duration="1000" />
            <item
                    drawable="arotate7"
                    duration="1000" />
            <item
                    drawable="down3"
                    duration="3000" />
            <repeat-item repeatCount="6">
                <item
                        drawable="arotate5"
                        duration="200" />
                <item
                        drawable="arotate6"
                        duration="200" />
                <item
                        drawable="arotate4"
                        duration="200" />
                <item
                        drawable="arotate3"
                        duration="200" />
            </repeat-item>
            <item
                    drawable="down3"
                    duration="5000" />
        </repeat-item>
    </motion>
    <motion
            checkMove="true"
            nextState="stop"
            state="awake">
        <item
                drawable="arotate7"
                duration="750" />
        <item
                drawable="down3"
                duration="750" />
    </motion>
    <motion state="moveUp">
        <repeat-item>
            <item
                    drawable="up1"
                    duration="250" />
            <item
                    drawable="up2"
                    duration="250" />
            <item
                    drawable="up3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveDown">
        <repeat-item>
            <item
                    drawable="down1"
                    duration="250" />
            <item
                    drawable="down2"
                    duration="250" />
            <item
                    drawable="down3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveLeft">
        <repeat-item>
            <item
                    drawable="left1"
                    duration="250" />
            <item
                    drawable="left2"
                    duration="250" />
            <item
                    drawable="left3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveRight">
        <repeat-item>
            <item
                    drawable="right1"
                    duration="250" />
            <item
                    drawable="right2"
                    duration="250" />
            <item
                    drawable="right3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveUpLeft">
        <repeat-item>
            <item
                    drawable="upleft1"
                    duration="250" />
            <item
                    drawable="upleft2"
                    duration="250" />
            <item
                    drawable="upleft3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveUpRight">
        <repeat-item>
            <item
                    drawable="upright1"
                    duration="250" />
            <item
                    drawable="upright2"
                    duration="250" />
            <item
                    drawable="upright3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveDownRight">
        <repeat-item>
            <item
                    drawable="dwright1"
                    duration="250" />
            <item
                    drawable="dwright2"
                    duration="250" />
            <item
                    drawable="dwright3"
                    duration="250" />
        </repeat-item>
    </motion>
    <motion state="moveDownLeft">
        <repeat-item>
            <item
                    drawable="dwleft1"
                    duration="250" />
            <item
                    drawable="dwleft2"
                    duration="250" />
            <item
                    drawable="dwleft3"
                    duration="250" />
        </repeat-item>
    </motion>
</motion-params>
