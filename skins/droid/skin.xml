<?xml version="1.0" encoding="utf-8"?>
<motion-params
        package="org.nqmgaming.droid"
        name="Droid"
        preview="icon.png"
        author="nqmgaming"
        acceleration="100"
        awakeState="awake"
        deaccelerationDistance="60"
        initialState="stop"
        maxVelocity="50"
        moveStatePrefix="move"
        proximityDistance="10">
    <motion state="stop">
        <repeat-item>
            <repeat-item repeatCount="2">
                <item
                        drawable="down1"
                        duration="5000" />
                <item
                        drawable="hrotate1"
                        duration="125" />
                <item
                        drawable="hrotate2"
                        duration="100" />
                <item
                        drawable="hrotate3"
                        duration="80" />
                <item
                        drawable="up1"
                        duration="80" />
                <item
                        drawable="hrotate4"
                        duration="80" />
                <item
                        drawable="hrotate5"
                        duration="100" />
                <item
                        drawable="hrotate6"
                        duration="125" />
            </repeat-item>
            <item
                    drawable="down1"
                    duration="2000" />
            <item
                    drawable="arotate1"
                    duration="125" />
            <item
                    drawable="arotate2"
                    duration="100" />
            <item
                    drawable="arotate3"
                    duration="80" />
            <item
                    drawable="arotate4"
                    duration="80" />
            <item
                    drawable="arotate5"
                    duration="100" />
