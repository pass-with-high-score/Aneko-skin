                        drawable="arotate5"
                        duration="100" />
                <item
                        drawable="arotate4"
                        duration="125" />
                <item
                        drawable="arotate5"
                        duration="100" />
            </repeat-item>
            <item
                    drawable="arotate6"
                    duration="125" />
            <item
                    drawable="arotate5"
                    duration="100" />
            <item
                    drawable="arotate4"
                    duration="80" />
            <item
                    drawable="arotate3"
                    duration="80" />
            <item
                    drawable="arotate2"
                    duration="100" />
            <item
                    drawable="arotate1"
                    duration="125" />
            <item
                    drawable="down1"
                    duration="10000" />
        </repeat-item>
    </motion>
    <motion
            checkMove="true"
            nextState="stop"
            state="awake">
        <item
                drawable="down1"
                duration="250" />
        <item
                drawable="hrotate1"
                duration="125" />
